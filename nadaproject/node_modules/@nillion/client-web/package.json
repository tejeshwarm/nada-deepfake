{
  "name": "@nillion/client-web",
  "version": "0.5.0",
  "description": "Nillion's javascript client for browsers",
  "author": "devsupport@nillion.com",
  "homepage": "https://docs.nillion.com/",
  "keywords": [],
  "license": "Apache-2.0",
  "browser": "/dist/nillion_client_wasm.js",
  "types": "/dist/nillion_client_wasm.d.ts",
  "exports": {
    ".": {
      "types": "./dist/nillion_client_wasm.d.ts",
      "import": "./dist/nillion_client_wasm.js",
      "require": "./dist/nillion_client_wasm.js"
    },
    "./proto": {
      "types": "./dist/proto.d.ts",
      "import": "./dist/proto.js",
      "require": "./dist/proto.js"
    }
  },
  "scripts": {
    "proto:generate": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src ./proto/nilchain.proto",
    "prebuild": "npm run proto:generate",
    "build": "npx webpack",
    "postbuild": "tsc && mv dist/index.d.ts dist/proto.d.ts",
    "jsdoc": "jsdoc -c jsdoc.json"
  },
  "dependencies": {
    "protobufjs": "^7.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@tsconfig/recommended": "^1.0.6",
    "babel-loader": "^9.1.3",
    "better-docs": "^2.7.3",
    "copy-webpack-plugin": "^12.0.2",
    "core-js": "^3.37.1",
    "jsdoc": "^4.0.3",
    "prettier": "^3.3.2",
    "ts-proto": "^1.178.0",
    "typescript": "^5.4.5",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "sideEffects": [
    "dist/snippets/**/*"
  ],
  "files": [
    "README.md",
    "dist",
    "package.json"
  ]
}
